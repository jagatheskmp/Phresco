            <resource>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>env-config_*.xml</exclude>
                    <exclude>log4j-*.*</exclude>
                </excludes>
            </resource>
        </resources>
        <directory>do_not_checkin/target</directory>
