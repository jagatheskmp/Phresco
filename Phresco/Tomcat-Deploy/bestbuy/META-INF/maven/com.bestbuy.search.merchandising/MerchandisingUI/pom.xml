                    <artifactId>lifecycle-mapping</artifactId>
                    <version>1.0.0</version>
                    <configuration>
                        <lifecycleMappingMetadata>
                            <pluginExecutions>
<pluginExecution>
    <pluginExecutionFilter>
        <groupId>
											org.apache.maven.plugins
										</groupId>
        <artifactId>
											maven-antrun-plugin
										</artifactId>
        <versionRange>
											[1.3,)
										</versionRange>
        <goals>
            <goal>run</goal>
										</goals>
									</pluginExecutionFilter>
    <action>
        <ignore/>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
