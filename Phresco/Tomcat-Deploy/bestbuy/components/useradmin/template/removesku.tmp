{{#if removeSku.length}}
	<table id="removeSkuTable" class="table table-striped table-bordered tab_table" cellpadding="0" cellspacing="0" border="0">
		<thead>
			<tr>
				<th>SKU ID</th>
				<th>User</th>
				<th>Date / Time</th>
			</tr>
		</thead>
		<tbody>
			{{#each removeSku}}
			<tr>
				<td>{{skuId}}</td>
				<td>{{userId}}</td>
				<td class="center_td">{{time}}</span></td>
			</tr>
			{{/each}}
		</tbody>
	</table>
{{/if}}