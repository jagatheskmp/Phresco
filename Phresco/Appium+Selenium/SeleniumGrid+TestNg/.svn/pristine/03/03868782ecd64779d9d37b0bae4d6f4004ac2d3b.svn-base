<?xml version="1.0" encoding="UTF-8"?>

<suite name="AllTestinDiffPlatform" parallel="tests" thread-count="5"
	verbose="3">

	<test name="VerifyEshopAppFirefox" junit="false" preserve-order="true">

		<parameter name="browser" value="firefox" />
		<parameter name="platform" value="WINDOWS" />

		<classes>
			<class name="com.photon.phresco.testcases.Eshop" />
		</classes>

	</test>

	<test name="VerifyEshopAppMAC" junit="false" preserve-order="true">

		<parameter name="browser" value="firefox" />
		<parameter name="platform" value="MAC" />

		<classes>
			<class name="com.photon.phresco.testcases.Eshop" />
		</classes>

	</test>

	<test name="VerifyEshopAppsafariMAC" junit="false" preserve-order="true">

		<parameter name="browser" value="safari" />
		<parameter name="platform" value="MAC" />

		<classes>
			<class name="com.photon.phresco.testcases.Eshop" />
		</classes>

	</test>

</suite>
 

 	
 
